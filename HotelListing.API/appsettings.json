	"Serilog": {
		"MinimumLevel": {
			"Default": "Warning",
			"Override": {
				"Microsoft": "Warning",
				"Microsoft.Hosting.Lifetime": "Information"
			}
		},
		"WriteTo": [
			{
				"Name": "File",
				"Args": {
					"path": "./logs/log-.txt",
					"rollingInterval": "Day"
				}
			},
			{
				"Name": "File",
				"Args": {
					"serverUrl": "http://localhost:5341"
				}
			}
		]
	},

	"AllowedHosts": "*"
}
